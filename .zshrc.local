alias gputemp="nvidia-smi --query-gpu=temperature.gpu --format=csv,noheader"

alias l='ls -GFh'
alias ll='l -l'
alias ls='ls -F'

alias fndate="date +%Y-%m-%d--%H-%M-%S"

